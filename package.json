{
  "name": "scraper",
  "version": "1.0.0",
  "description": "A TypeScript-based web scraper that searches for specific code files across Bitbucket repositories and automatically clones matching repositories",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "bitbucket-scraper": "./index.ts"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx index.ts",
    "start": "node dist/index.js",
    "cli": "tsx index.ts",
    "scraper": "tsx index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "bitbucket",
    "scraper",
    "git",
    "clone",
    "search",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.4.1",
  "dependencies": {
    "@1password/op-js": "^0.1.0",
    "dotenv": "^16.0.0",
    "otpauth": "^9.2.2",
    "playwright": "^1.54.1",
    "winston": "^3.17.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/yargs": "^17.0.33",
    "tsx": "^4.0.0",
    "typescript": "^5.8.3"
  }
}